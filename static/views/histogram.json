{
	"viewName": "histogram",
  "prototypes": "basic_bar",
	"plotOptions": {
      "series": {
        "animation": false
      }
    },
	"viewDefinition": {
		"title": {
			"text": "{{market}}"
		},
		"xAxis": {
			"type": "datetime",
            "dateTimeLabelFormats": {"month": "%b %Y"}
			},
		"yAxis": [
          {
            "title": {
              "text": "Average Return"
            }
          },
          {
				"opposite": true
			}],
		"series": [{
			"name": "Histogram",
			"type": "column",
			"pointPadding": 0,
			"groupPadding": 0,
			"pointPlacement": "between"
		}]
	}
}
